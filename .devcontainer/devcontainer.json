{
    "name": "DevContainer",
    "build": {
      "dockerfile": "./.devcontainer/Dockerfile",
      "context": ".."
    },
    "settings": {
      "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter",
      "ms-azuretools.vscode-docker"
    ],
    "postCreateCommand": "make install",
    "remoteUser": "vscode"
  }